//script1124(1124)(int arg0)(void)
int ivar1;
ivar1 = if_gettrans(arg0);
if (load_varc(992) == 1) {
	ivar1 = calc(ivar1 + 1);
	if (ivar1 >= 255) 
		store_varc(992, 0)
}
else {
	ivar1 = calc(1 - ivar1);
	if (ivar1 <= 0) 
		store_varc(992, 1)
}
if (ivar1 > 255) 
	ivar1 = 255;
else {
	if (ivar1 < 0) 
		ivar1 = 0;
}
if_settrans(ivar1, arg0);
return;